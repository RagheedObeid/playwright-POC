{
  "name": "my-vue-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "wait-on": "wait-on http://localhost:8080",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "concurrently -k \"npm:serve\" \"npm:wait-on\" \"npm:test:playwright\"",
    "test:playwright": "playwright test",
    "posttest:e2e": "kill-port 8080"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@playwright/test": "^1.46.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "kill-port": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.4",
    "wait-on": "^7.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
